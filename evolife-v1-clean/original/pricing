<!-- START - Rewardful get param carry over --> 

<script>
// Function to sanitize query parameters (basic example for removing potentially harmful content)
function sanitizeInput(input) {
    // Creating a temporary element to use its innerHTML for sanitization
    let tempDiv = document.createElement('div');
    tempDiv.textContent = input;  // Using textContent to escape any HTML tags or JavaScript
    return tempDiv.innerHTML;  // Returns the sanitized string
}

// Function to set a cookie
function setCookie(name, value, days) {
    const date = new Date();
    date.setTime(date.getTime() + (days*24*60*60*1000));
    const expires = "expires=" + date.toUTCString();
    document.cookie = name + "=" + value + ";" + expires + ";path=/";
}

// Function to get a cookie
function getCookie(name) {
    const nameEQ = name + "=";
    const ca = document.cookie.split(';');
    for(let i=0; i < ca.length; i++) {
        let c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}
  
function toggleBanners(showId) {
  
  const allDefaultBanners = document.querySelectorAll('[id="default_floating_banner"]');
  allDefaultBanners.forEach(banner => { banner.style.setProperty("display", "none", "important"); });
  
  
    const targetBanner = document.getElementById(showId);
    if (targetBanner) targetBanner.style.display = "flex";
}

// Retrieve query parameters from the URL
let queryParams = new URLSearchParams(window.location.search);

// Sanitize each query parameter before using it
queryParams.forEach((value, key) => {
    queryParams.set(key, sanitizeInput(value));  // Sanitize each query parameter
});

let sanitizedQueryParams = queryParams.toString();
  

// If sanitized query parameters are available, store them in a cookie for 1 day
if (sanitizedQueryParams) {
    setCookie('queryParams', sanitizedQueryParams, 1);
} else {
    // Use the stored sanitized query parameters if available
    sanitizedQueryParams = getCookie('queryParams');
}

const urlParamsForCampaign = new URLSearchParams(sanitizedQueryParams);
const utmCampaign = urlParamsForCampaign.get("utm_campaign");
const viaParam = urlParamsForCampaign.get("via");
if(!!viaParam){
  toggleBanners("-");
}else if (utmCampaign === "25") {
	toggleBanners("forbes_floating_banner");
} else if (utmCampaign === "115") {
    toggleBanners("expertise_floating_banner");
}

// Select all <a> tags in the document
const links = document.querySelectorAll('a');

// Iterate over each <a> tag
links.forEach(link => {
    // Check if the href attribute contains the 'FRIDAYS_QUERY_PARAMS' string
    if (link.href.includes('FRIDAYS_QUERY_PARAMS')) {
        // If sanitized query parameters are available, replace 'FRIDAYS_QUERY_PARAMS' with actual query params
        if (sanitizedQueryParams) {
            // Check if the link already has query parameters
            if (link.href.includes('?')) {
                // If there are existing parameters, add the new ones using '&'
                link.href = link.href.replace('FRIDAYS_QUERY_PARAMS', '&' + sanitizedQueryParams);
            } else {
                // If no query parameters exist, add '?' to start the query string
                link.href = link.href.replace('FRIDAYS_QUERY_PARAMS', '?' + sanitizedQueryParams);
            }
        } else {
            // Remove 'FRIDAYS_QUERY_PARAMS' if no parameters are found
            link.href = link.href.replace('FRIDAYS_QUERY_PARAMS', "");
        }
    }
});
</script>


<!-- END - Rewardful get param carry over --> 

