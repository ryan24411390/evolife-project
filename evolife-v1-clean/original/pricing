// UTM Parameter Display Controller - Robust Version
// Shows element with ID "mp-background" and hides "zepbound" and "ozempic" when specific parameters are present

(function() {
    'use strict';
    
    console.log('Script starting...');
    
    // Function to get URL parameters
    function getURLParameter(name) {
        try {
            const urlParams = new URLSearchParams(window.location.search);
            return urlParams.get(name);
        } catch (error) {
            console.error('Error getting URL parameter:', error);
            return null;
        }
    }
    
    // Function to check if required parameters are present
    function checkParameters() {
        console.log('Checking parameters...');
        console.log('Current URL:', window.location.href);
        console.log('Search params:', window.location.search);
        
        try {
            // Check for UTM parameters (everflow affiliate)
            const utmParams = {
                utm_source: 'everflow',
                utm_medium: 'affiliate'
            };
            
            let utmMatch = true;
            for (const [param, expectedValue] of Object.entries(utmParams)) {
                const actualValue = getURLParameter(param);
                console.log(`UTM check - ${param}: expected "${expectedValue}", got "${actualValue}"`);
                if (actualValue !== expectedValue) {
                    utmMatch = false;
                }
            }
            console.log('UTM match result:', utmMatch);
            
            // Check for alternative parameters (oid, affid, sub2)
            const altParams = ['oid', 'affid', 'sub2'];
            let altMatch = true;
            for (const param of altParams) {
                const value = getURLParameter(param);
                console.log(`Alt param check - ${param}: "${value}"`);
                if (!value) {
                    altMatch = false;
                }
            }
            console.log('Alt params match result:', altMatch);
            
            // Return true if either parameter set is complete
            const finalResult = utmMatch || altMatch;
            console.log('Final check result:', finalResult);
            return finalResult;
        } catch (error) {
            console.error('Error in checkParameters:', error);
            return false;
        }
    }
    
    // Function to show/hide elements based on parameters
    function updateElements() {
        console.log('Updating elements...');
        
        try {
            // Show mp-background element
            const mpBackground = document.getElementById('mp-background');
            if (mpBackground) {
                mpBackground.style.display = 'flex';
                console.log('✅ mp-background element set to flex');
            } else {
                console.warn('❌ Element with ID "mp-background" not found');
            }
            
            // Hide zepbound and ozempic elements
            const elementsToHide = ['zepbound', 'ozempic'];
            elementsToHide.forEach(id => {
                const element = document.getElementById(id);
                if (element) {
                    element.style.display = 'none';
                    console.log(`✅ ${id} element set to none`);
                } else {
                    console.warn(`❌ Element with ID "${id}" not found`);
                }
            });
        } catch (error) {
            console.error('Error in updateElements:', error);
        }
    }
    
    // Main execution function
    function init() {
        console.log('Init function called');
        try {
            if (checkParameters()) {
                console.log('Parameters matched! Updating elements...');
                updateElements();
            } else {
                console.log('Parameters did not match');
            }
        } catch (error) {
            console.error('Error in init:', error);
        }
    }
    
    // Run immediately if DOM is ready, otherwise wait
    if (document.readyState === 'loading') {
        console.log('DOM still loading, waiting for DOMContentLoaded...');
        document.addEventListener('DOMContentLoaded', init);
    } else {
        console.log('DOM ready, running init immediately...');
        init();
    }
    
    // Also run on window load as backup
    window.addEventListener('load', function() {
        console.log('Window load event fired, running init as backup...');
        init();
    });
    
})();
</script></div></div><script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=66c8a0fb54f84ec4a09643c7" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script><script src="https://cdn.prod.website-files.com/66c8a0fb54f84ec4a09643c7/js/fridays-2024-da06be12dacaba199501f0724d.schunk.0c13dd10975c394b.js" type="text/javascript"></script><script src="https://cdn.prod.website-files.com/66c8a0fb54f84ec4a09643c7/js/fridays-2024-da06be12dacaba199501f0724d.schunk.529f806b94fc5614.js" type="text/javascript"></script><script src="https://cdn.prod.website-files.com/66c8a0fb54f84ec4a09643c7/js/fridays-2024-da06be12dacaba199501f0724d.schunk.5d79c96558ab085a.js" type="text/javascript"></script><script src="https://cdn.prod.website-files.com/66c8a0fb54f84ec4a09643c7/js/fridays-2024-da06be12dacaba199501f0724d.8244264a.ff185e9ab39804f3.js" type="text/javascript"></script><!-- Smoth Scroll -->
